<http://data.networkedplanet.com/data/charity_commission/ontology/> rdf:type owl:Ontology ;
                                                                    
                                                                    owl:imports <http://reference.data.gov.uk/def/intervals/> .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://data.networkedplanet.com/data/charity_commission/ontology/areaOfOperation

ont:areaOfOperation rdf:type owl:ObjectProperty ;
                    
                    rdfs:comment "Relates a Charity to the Area within which they provide benefit."@en ;
                    
                    rdfs:range ont:Area ;
                    
                    rdfs:domain ont:Charity .



###  http://data.networkedplanet.com/data/charity_commission/ontology/charitablePurpose

ont:charitablePurpose rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "Relates a Charity to its Charitable Puropse"@en ;
                      
                      rdfs:range ont:CharitablePurpose ;
                      
                      rdfs:domain ont:Charity .



###  http://data.networkedplanet.com/data/charity_commission/ontology/financialSummary

ont:financialSummary rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "Relates a Charity to a Financial Summary for that charity."@en ;
                     
                     rdfs:domain ont:Charity ;
                     
                     rdfs:range ont:FinancialSummary .



###  http://data.networkedplanet.com/data/charity_commission/ontology/financialYearEnd

ont:financialYearEnd rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "Relates a Financial Summary or Annual Return to the Calendar Day that marks the end of the financial year it covers."@en ;
                     
                     rdfs:range <http://reference.data.gov.uk/def/intervals/CalendarDay> ;
                     
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( ont:AnnualReturn
                                                 ont:FinancialSummary
                                               )
                                 ] .



###  http://data.networkedplanet.com/data/charity_commission/ontology/financialYearStart

ont:financialYearStart rdf:type owl:ObjectProperty ;
                       
                       rdfs:comment "Relates a Financial Summary or Annual Return to the Calendar Day that marks the start of the financial year covered."@en ;
                       
                       rdfs:range <http://reference.data.gov.uk/def/intervals/CalendarDay> ;
                       
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( ont:AnnualReturn
                                                   ont:FinancialSummary
                                                 )
                                   ] .



###  http://data.networkedplanet.com/data/charity_commission/ontology/question

ont:question rdf:type owl:ObjectProperty ;
             
             rdfs:comment "Relates a Summary Information Response to the Summary Information Question that it provides a response to."@en ;
             
             rdfs:range ont:SummaryInformationQuestion ;
             
             rdfs:domain ont:SummaryInformationResponse .



###  http://data.networkedplanet.com/data/charity_commission/ontology/registerStatus

ont:registerStatus rdf:type owl:ObjectProperty ;
                   
                   rdfs:comment """Relates a Charity to its registration status.

The resource http://data.networkedplanet.com/data/charity_commission/ontology/registerStatus/REGISTERED is used as the value of this property for charities whose current status is registered. The resource http://data.networkedplanet.com/data/charity_commission/ontology/registerStatus/REMOVED is used as the value of this property for charities that have been removed from the register."""@en ;
                   
                   rdfs:domain ont:Charity .



###  http://data.networkedplanet.com/data/charity_commission/ontology/registrationDate

ont:registrationDate rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "Date of registration of a Charity."@en ;
                     
                     rdfs:domain ont:Charity ;
                     
                     rdfs:range <http://reference.data.gov.uk/def/intervals/CalendarDay> .



###  http://data.networkedplanet.com/data/charity_commission/ontology/removalDate

ont:removalDate rdf:type owl:ObjectProperty ;
                
                rdfs:comment "Date of removal of a Charity from the register"@en ;
                
                rdfs:domain ont:Charity ;
                
                rdfs:range <http://reference.data.gov.uk/def/intervals/CalendarDay> .



###  http://data.networkedplanet.com/data/charity_commission/ontology/removalReason

ont:removalReason rdf:type owl:ObjectProperty ;
                  
                  rdfs:comment "Relates a Charity that has been removed from the register to the reason for its removal."@en ;
                  
                  rdfs:domain ont:Charity ;
                  
                  rdfs:range ont:RemovalReason .



###  http://data.networkedplanet.com/data/charity_commission/ontology/returnCycle

ont:returnCycle rdf:type owl:ObjectProperty ;
                
                rdfs:comment "Relates a Finanal Accounts or Annual Return to the Return Cycle that the accounts or return apply to."@en ;
                
                rdfs:range ont:ReturnCycle ;
                
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( ont:AnnualReturn
                                            ont:FinancialAccounts
                                          )
                            ] .



###  http://data.networkedplanet.com/data/charity_commission/ontology/sirCycle

ont:sirCycle rdf:type owl:ObjectProperty ;
             
             rdfs:comment "Relates a Summary Information Return to the SIR Cycle period that the return is provided for."@en ;
             
             rdfs:range ont:SIRCycle ;
             
             rdfs:domain ont:SummaryInformationReturn .



###  http://data.networkedplanet.com/data/charity_commission/ontology/submissionDate

ont:submissionDate rdf:type owl:ObjectProperty ;
                   
                   rdfs:comment "Relates an Accounts Submission or Annual Return Submission to the Interval that represents the day on which the submission was made."@en ;
                   
                   rdfs:range <http://reference.data.gov.uk/def/intervals/CalendarDay> ;
                   
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( ont:AccountsSubmission
                                               ont:AnnualReturnSubmission
                                             )
                               ] .



###  http://data.networkedplanet.com/data/charity_commission/ontology/submittedAccounts

ont:submittedAccounts rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "Relates an Accounts Submission to the Financial Accounts provided by the submission."@en ;
                      
                      rdfs:domain ont:AccountsSubmission ;
                      
                      rdfs:range ont:FinancialAccounts .



###  http://data.networkedplanet.com/data/charity_commission/ontology/submittedBy

ont:submittedBy rdf:type owl:ObjectProperty ;
                
                rdfs:comment "Relates a Summary Information Return or Annual Return to the Charity that provided the return."@en ;
                
                rdfs:range ont:Charity ;
                
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( ont:AnnualReturn
                                            ont:SummaryInformationReturn
                                          )
                            ] .



###  http://data.networkedplanet.com/data/charity_commission/ontology/submittedReturn

ont:submittedReturn rdf:type owl:ObjectProperty ;
                    
                    rdfs:comment "Relates an Annual Return Submission to the Annual Return that was provided by the Charity."@en ;
                    
                    rdfs:range ont:AnnualReturn ;
                    
                    rdfs:domain ont:AnnualReturnSubmission .



###  http://schema.org/childOrganization

<http://schema.org/childOrganization> rdf:type owl:ObjectProperty ;
                                      
                                      rdfs:domain <http://schema.org/Organization> ;
                                      
                                      rdfs:range <http://schema.org/Organization> ;
                                      
                                      owl:inverseOf <http://schema.org/parentOrganization> .



###  http://schema.org/containedInPlace

<http://schema.org/containedInPlace> rdf:type owl:ObjectProperty ,
                                              owl:TransitiveProperty ;
                                     
                                     rdfs:comment "Relates an Area to an Area within which it is situated."@en ;
                                     
                                     rdfs:domain ont:Area ;
                                     
                                     rdfs:range ont:Area .



###  http://schema.org/parentOrganization

<http://schema.org/parentOrganization> rdf:type owl:ObjectProperty .





